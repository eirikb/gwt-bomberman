<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:lobby="urn:import:no.eirikb.bomberman.client.ui.lobby">
    <g:HTMLPanel>
        Info: <g:Label ui:field="infoLabel" />
        <lobby:GameWaitPanel ui:field="gameWaitPanel" visible="false" />
        <g:TabPanel ui:field="tabPanel" animationEnabled="true">
            <g:Tab text="Games">
                <lobby:GameListPanel ui:field="gameListPanel" />
            </g:Tab>
            <g:Tab text="Create game">
                <lobby:GameCreatePanel ui:field="gameCreatePanel" />
            </g:Tab>
        </g:TabPanel>
    </g:HTMLPanel>
</ui:UiBinder>